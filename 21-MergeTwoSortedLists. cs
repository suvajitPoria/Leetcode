/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     public int val;
 *     public ListNode next;
 *     public ListNode(int val=0, ListNode next=null) {
 *         this.val = val;
 *         this.next = next;
 *     }
 * }
 */
public class Solution {
    ListNode newList = null;  

    public ListNode MergeTwoLists(ListNode list1, ListNode list2) {
        if(list1 != null) Traverse(list1);
        if(list2 != null) Traverse(list2);
        return newList;
    }

    public void Traverse(ListNode tempList)
    {
        while(tempList != null)
        {
            if(newList == null)
            {
                newList = new ListNode(tempList.val);
            }
            else
            {
                ListNode temp = newList;
                ListNode prev = null;

                while(temp != null && temp.val < tempList.val)
                {
                    prev = temp;
                    temp = temp.next;
                }
                if(prev == null)
                {
                    newList = new ListNode(tempList.val, newList);
                }
                else
                {
                    prev.next = new ListNode(tempList.val, temp);
                }
            }
            tempList = tempList.next;
        }
    }
}
